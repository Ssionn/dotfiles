font_family MonaspaceNeon-WideRegular
bold_font MonaspaceNeon-WideBold
italic_font MonaspaceNeon-WideItalic
bold_italic_font MonaspaceNeon-WideBoldItalic

# font_family JetBrainsMono Nerd Font
# bold_font JetBrainsMono Nerd Font Bold
# italic_font JetBrainsMono Nerd Font Italic
# bold_italic_font JetBrainsMono Nerd Font Bold Italic

font_size 13
adjust_line_height 125%

cursor_blink_interval 0
remember_window_size yes
initial_window_width 1536
initial_window_height 864
enable_audio_bell no

# BEGIN_KITTY_THEME
# GitHub Dark
include current-theme.conf
# END_KITTY_THEME

